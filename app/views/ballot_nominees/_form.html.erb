<%= simple_form_for(@ballot_nominee) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :video_id %>
    <%= f.input :category_key %>
    <%= f.input :bn_type %>
    <%= f.input :nominee %>
    <%= f.input :nominee_description %>
    <%= f.association :ballot %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
